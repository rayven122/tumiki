{
  "mcpServers": {
    "github": {
      "enabled": true,
      "name": "GitHub MCP Server",
      "url": "https://github-mcp.example.com/sse",
      "authType": "bearer",
      "authToken": "${GITHUB_TOKEN}",
      "headers": {}
    },
    "slack": {
      "enabled": false,
      "name": "Slack MCP Server",
      "url": "https://slack-mcp.example.com/sse",
      "authType": "bearer",
      "authToken": "${SLACK_TOKEN}",
      "headers": {}
    },
    "postgres": {
      "enabled": true,
      "name": "PostgreSQL MCP Server",
      "url": "https://postgres-mcp.example.com/sse",
      "authType": "api_key",
      "authToken": "${POSTGRES_API_KEY}",
      "headers": {
        "X-Custom-Header": "value"
      }
    },
    "custom": {
      "enabled": false,
      "name": "Custom MCP Server",
      "url": "https://custom-mcp.example.com/sse",
      "authType": "none",
      "headers": {}
    }
  }
}

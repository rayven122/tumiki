{
  "name": "@tumiki/mcp-proxy",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm with-env tsx watch src/index.ts",
    "build": "pnpm build:clean && tsc --project tsconfig.build.json",
    "build:clean": "rm -rf dist .cache",
    "start": "pnpm with-env node dist/src/index.js",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "with-env-test": "dotenv -e ../../.env.test --",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:seed": "pnpm with-env-test tsx test/seed.ts",
    "test:verify": "pnpm with-env-test tsx test/verify.ts",
    "benchmark": "bash benchmark/scripts/run-benchmark.sh"
  },
  "dependencies": {
    "@google-cloud/dlp": "^6.5.0",
    "@google-cloud/pubsub": "^5.2.0",
    "@hono/node-server": "^1.19.6",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@toon-format/toon": "^2.1.0",
    "@tumiki/db": "workspace:*",
    "@tumiki/license": "workspace:*",
    "@tumiki/oauth-token-manager": "workspace:*",
    "@tumiki/prompts": "workspace:*",
    "@tumiki/shared": "workspace:*",
    "ai": "6.0.103",
    "fetch-to-node": "^2.1.0",
    "google-auth-library": "^10.5.0",
    "hono": "^4.11.10",
    "jose": "^6.1.2",
    "node-cron": "^4.2.1",
    "openid-client": "^6.3.3",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@tumiki/vitest-config": "workspace:*",
    "@types/node": "catalog:",
    "@types/node-cron": "^3.0.11",
    "@vitest/coverage-v8": "catalog:",
    "dotenv-cli": "^8.0.0",
    "eslint": "catalog:",
    "eslint-plugin-headers": "^1.3.3",
    "fast-check": "^4.5.3",
    "tsx": "^4.21.0",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}

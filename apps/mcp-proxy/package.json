{
  "name": "@tumiki/mcp-proxy",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm with-env tsx watch src/index.ts",
    "build": "pnpm build:clean && tsc --project tsconfig.build.json",
    "build:clean": "rm -rf dist .cache",
    "start": "pnpm with-env node dist/index.js",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "with-env-test": "dotenv -e ../../.env.test --",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:seed": "pnpm with-env-test tsx test/seed.ts",
    "test:verify": "pnpm with-env-test tsx test/verify.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.6",
    "@modelcontextprotocol/sdk": "^1.24.0",
    "@tumiki/db": "workspace:*",
    "@tumiki/oauth-token-manager": "workspace:*",
    "fetch-to-node": "^2.1.0",
    "google-auth-library": "^10.5.0",
    "hono": "^4.10.7",
    "redis": "^5.10.0"
  },
  "devDependencies": {
    "@tumiki/vitest-config": "workspace:*",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "dotenv-cli": "^8.0.0",
    "eslint": "catalog:",
    "tsx": "^4.21.0",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}

{
  "name": "@tumiki/mcp-proxy",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm with-env tsx watch src/index.ts",
    "build": "pnpm build:clean && tsc --project tsconfig.build.json",
    "build:clean": "rm -rf dist .cache",
    "start": "pnpm with-env node dist/index.js",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "@modelcontextprotocol/sdk": "^1.10.1",
    "@tumiki/db": "workspace:*",
    "@upstash/redis": "^1.34.4",
    "hono": "^4.7.4",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@tumiki/vitest-config": "workspace:*",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "dotenv-cli": "^8.0.0",
    "eslint": "catalog:",
    "tsx": "^4.19.2",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}

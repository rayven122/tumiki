{
  "name": "@tumiki/proxy-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm with-env pnpm build && pnpm start",
    "build": "pnpm build:clean && tsc && chmod +x build/src/index.js",
    "build:clean": "rm -rf build .cache",
    "start": "pnpm with-env node build/src/index.js",
    "pm2:start": "pnpm with-env pm2 start ecosystem.config.cjs",
    "pm2:logs": "pm2 logs tumiki-proxy-server",
    "pm2:logs:remote": "./scripts/stream-pm2-logs.sh",
    "pm2:logs:download": "./scripts/download-pm2-logs.sh",
    "pm2:logs:remote:out": "./scripts/stream-pm2-logs.sh --out",
    "pm2:logs:remote:err": "./scripts/stream-pm2-logs.sh --err",
    "pm2:status": "pm2 status",
    "pm2:status:remote": "./scripts/stream-pm2-logs.sh --status",
    "pm2:restart": "pm2 restart tumiki-proxy-server",
    "pm2:stop": "pm2 stop tumiki-proxy-server",
    "pm2:delete": "pm2 delete tumiki-proxy-server",
    "clean": "git clean -xdf .cache .turbo node_modules build",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "format:fix": "prettier --write . --ignore-path ../../.gitignore",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "verify": "pnpm with-env ./scripts/test-mcp-inspector.sh",
    "inspector": "pnpm exec mcp-inspector",
    "test:instance-id": "pnpm with-env npx tsx scripts/test-instance-id-auth.ts",
    "test:sse-instance-id": "pnpm with-env npx tsx scripts/test-sse-instance-id-auth.ts",
    "test:connection": "pnpm with-env npx tsx scripts/test-connection.ts",
    "test:connection:query": "pnpm with-env TEST_MODE=query npx tsx scripts/test-connection.ts",
    "test:connection:instance-id": "pnpm with-env TEST_MODE=instance-id npx tsx scripts/test-connection.ts",
    "perf:test": "pnpm with-env npx tsx scripts/perf-test.ts",
    "perf:dashboard": "npx tsx watch scripts/dashboard-server.ts"
  },
  "dependencies": {
    "@brave/brave-search-mcp-server": "^1.3.7",
    "@googlemaps/code-assist-mcp": "^0.1.5",
    "@line/line-bot-mcp-server": "^0.3.0",
    "@microsoft/clarity-mcp-server": "^1.0.5",
    "@modelcontextprotocol/sdk": "^1.10.1",
    "@modelcontextprotocol/server-github": "^2025.4.8",
    "@modelcontextprotocol/server-sequential-thinking": "^2025.7.1",
    "@playwright/mcp": "^0.0.29",
    "@stripe/mcp": "^0.2.4",
    "@suekou/mcp-notion-server": "^1.2.4",
    "@tumiki/db": "workspace:*",
    "@tumiki/youtube-mcp": "workspace:*",
    "@upstash/context7-mcp": "^1.0.14",
    "agents": "^0.0.45",
    "deepl-mcp-server": "^0.1.3-beta.0",
    "discord-mcp-server": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-oauth2-jwt-bearer": "^1.6.1",
    "fastmcp": "^1.27.6",
    "figma-developer-mcp": "^0.5.0",
    "hono": "^4.7.4",
    "just-pick": "^4.2.0",
    "lru-cache": "^11.2.1",
    "marked": "^15.0.8",
    "mcp-gemini-google-search": "^0.1.1",
    "mcp-server-gsc": "^0.2.0",
    "mcp-server-kubernetes": "^2.9.1",
    "microcms-mcp-server": "^0.3.6",
    "n8n-mcp": "^2.10.6",
    "octokit": "^4.1.2",
    "puppeteer-mcp-server": "^0.7.2",
    "slack-mcp-server": "^1.1.16",
    "task-master-ai": "^0.18.0",
    "workers-mcp": "0.1.0-3",
    "ws": "^8.18.2",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.16.6",
    "@tumiki/vitest-config": "workspace:*",
    "@types/autocannon": "^7.12.7",
    "@types/express": "^5.0.2",
    "@types/express-serve-static-core": "^5.0.7",
    "@types/qs": "^6.14.0",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@typescript/native-preview": "7.0.0-dev.20250814.1",
    "@vitest/coverage-v8": "catalog:",
    "autocannon": "^8.0.0",
    "concurrently": "^9.0.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "catalog:",
    "supertest": "^7.1.4",
    "typescript-eslint": "^8.33.0",
    "vitest": "catalog:",
    "vitest-mock-extended": "^3.1.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}

{
  "name": "@tumiki/proxy-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm with-env pnpm build && pnpm start",
    "build": "tsc && chmod +x build/index.js",
    "start": "pnpm with-env node build/index.js",
    "start:prod": "bash start-prod.sh",
    "pm2:logs": "pm2 logs tumiki-proxy-server",
    "pm2:status": "pm2 status",
    "pm2:restart": "pm2 restart tumiki-proxy-server",
    "pm2:stop": "pm2 stop tumiki-proxy-server",
    "pm2:delete": "pm2 delete tumiki-proxy-server",
    "clean": "git clean -xdf .cache .turbo node_modules build",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "format:fix": "prettier --write . --ignore-path ../../.gitignore",
    "inspector": " NODE_TLS_REJECT_UNAUTHORIZED=0 pnpm dlx @modelcontextprotocol/inspector http://localhost:8080/sse"
  },
  "dependencies": {
    "@cloudflare/workers-oauth-provider": "^0.0.2",
    "@modelcontextprotocol/sdk": "^1.10.1",
    "@modelcontextprotocol/server-github": "^2025.4.8",
    "@playwright/mcp": "^0.0.29",
    "@suekou/mcp-notion-server": "^1.2.4",
    "@tumiki/db": "workspace:*",
    "@upstash/context7-mcp": "^1.0.14",
    "agents": "^0.0.45",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "fastmcp": "^1.27.6",
    "figma-developer-mcp": "^0.4.3",
    "hono": "^4.7.4",
    "just-pick": "^4.2.0",
    "marked": "^15.0.8",
    "octokit": "^4.1.2",
    "task-master-ai": "^0.18.0",
    "workers-mcp": "0.1.0-3",
    "ws": "^8.18.2",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/ws": "^8.18.1",
    "@vercel/ncc": "^0.38.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "catalog:",
    "typescript-eslint": "^8.33.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}

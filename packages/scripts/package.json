{
  "name": "@tumiki/scripts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./upsertAll": {
      "types": "./dist/upsertAll.d.ts",
      "default": "./dist/upsertAll.js"
    },
    "./upsertMcpServers": {
      "types": "./dist/upsertMcpServers.d.ts",
      "default": "./dist/upsertMcpServers.js"
    },
    "./upsertMcpTools": {
      "types": "./dist/upsertMcpTools.d.ts",
      "default": "./dist/upsertMcpTools.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "upsertAll": "pnpm with-env tsx src/upsertAll.ts",
    "delete-orgs": "pnpm with-env tsx src/deleteAllOrganizations.ts",
    "security:scan:mcp": "pnpm with-env tsx src/security-scan-mcp.ts",
    "with-env": "dotenv -e ../../.env --",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../../.gitignore --write",
    "test": "vitest"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@tumiki/db": "workspace:*",
    "@tumiki/shared": "workspace:*",
    "google-auth-library": "^10.5.0",
    "uuid": "^13.0.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@tumiki/eslint-config": "workspace:*",
    "@tumiki/prettier-config": "workspace:*",
    "@tumiki/tsconfig": "workspace:*",
    "@tumiki/vitest-config": "workspace:*",
    "@typescript/native-preview": "7.0.0-dev.20260301.1",
    "@vitest/coverage-v8": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "prettier": "@tumiki/prettier-config"
}

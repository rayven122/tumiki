{
  "name": "@tumiki/scripts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./upsertAll": {
      "types": "./dist/upsertAll.d.ts",
      "default": "./dist/upsertAll.js"
    },
    "./upsertMcpServers": {
      "types": "./dist/upsertMcpServers.d.ts",
      "default": "./dist/upsertMcpServers.js"
    },
    "./upsertMcpTools": {
      "types": "./dist/upsertMcpTools.d.ts",
      "default": "./dist/upsertMcpTools.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "upsertAll": "pnpm with-env tsx src/upsertAll.ts",
    "security:scan:mcp": "pnpm with-env tsx src/security-scan-mcp.ts",
    "with-env": "dotenv -e ../../.env --",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../../.gitignore --write",
    "test": "vitest"
  },
  "dependencies": {
    "@brave/brave-search-mcp-server": "^1.3.7",
    "@googlemaps/code-assist-mcp": "^0.1.7",
    "@line/line-bot-mcp-server": "^0.3.2",
    "@microsoft/clarity-mcp-server": "^1.0.5",
    "@modelcontextprotocol/server-github": "^2025.4.8",
    "@modelcontextprotocol/server-sequential-thinking": "^2025.11.25",
    "@playwright/mcp": "^0.0.29",
    "@stripe/mcp": "^0.2.5",
    "@suekou/mcp-notion-server": "^1.2.4",
    "@tumiki/db": "workspace:*",
    "@tumiki/youtube-mcp": "workspace:*",
    "@upstash/context7-mcp": "^1.0.31",
    "deepl-mcp-server": "0.1.3-beta.0",
    "discord-mcp-server": "^1.0.1",
    "figma-developer-mcp": "^0.5.2",
    "google-auth-library": "^10.5.0",
    "mcp-gemini-google-search": "^0.1.1",
    "mcp-server-gsc": "^0.2.0",
    "mcp-server-kubernetes": "^2.9.9",
    "microcms-mcp-server": "^0.3.7",
    "n8n-mcp": "^2.28.2",
    "puppeteer-mcp-server": "^0.7.2",
    "slack-mcp-server": "^1.1.26",
    "task-master-ai": "^0.18.0",
    "zod": "catalog:"

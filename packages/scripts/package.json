{
  "name": "@tumiki/scripts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./cleanUserMcp": {
      "types": "./dist/src/cleanUserMcp.d.ts",
      "default": "./dist/src/cleanUserMcp.js"
    },
    "./createUserMcpServer": {
      "types": "./dist/src/createUserMcpServer.d.ts",
      "default": "./dist/src/createUserMcpServer.js"
    },
    "./migrateWaitingList": {
      "types": "./dist/src/migrateWaitingList.d.ts",
      "default": "./dist/src/migrateWaitingList.js"
    },
    "./upsertAll": {
      "types": "./dist/src/upsertAll.d.ts",
      "default": "./dist/src/upsertAll.js"
    },
    "./upsertMcpServers": {
      "types": "./dist/src/upsertMcpServers.d.ts",
      "default": "./dist/src/upsertMcpServers.js"
    },
    "./upsertMcpTools": {
      "types": "./dist/src/upsertMcpTools.d.ts",
      "default": "./dist/src/upsertMcpTools.js"
    },
    "./test-auth0-api": {
      "types": "./dist/src/test-auth0-api.d.ts",
      "default": "./dist/src/test-auth0-api.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "upsertAll": "pnpm with-env-all tsx src/upsertAll.ts",
    "migrateWaitingList": "pnpm with-env tsx src/migrateWaitingList.ts",
    "test-auth0-api": "pnpm with-env tsx src/test-auth0-api.ts",
    "with-env": "dotenv -e ../../.env --",
    "with-env-upsert": "dotenv -e ../../.env.upsert --",
    "with-env-all": "dotenv -e ../../.env -- dotenv -e ../../.env.upsert --",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --check . --ignore-path ../../.gitignore --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@line/line-bot-mcp-server": "^0.3.0",
    "@modelcontextprotocol/server-github": "^2025.4.8",
    "@playwright/mcp": "^0.0.29",
    "@suekou/mcp-notion-server": "^1.2.4",
    "@tumiki/db": "workspace:*",
    "@tumiki/utils": "workspace:*",
    "@upstash/context7-mcp": "^1.0.14",
    "deepl-mcp-server": "^0.1.3-beta.0",
    "discord-mcp-server": "^1.0.1",
    "figma-developer-mcp": "^0.5.0",
    "mcp-gemini-google-search": "^0.1.1",
    "microcms-mcp-server": "^0.3.6",
    "puppeteer-mcp-server": "^0.7.2",
    "slack-mcp-server": "^1.1.16",
    "task-master-ai": "^0.18.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@tumiki/eslint-config": "workspace:*",
    "@tumiki/prettier-config": "workspace:*",
    "@tumiki/tsconfig": "workspace:*",
    "@tumiki/vitest-config": "workspace:*",
    "@vitest/coverage-v8": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsup": "^8.3.5",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "prettier": "@tumiki/prettier-config"
}

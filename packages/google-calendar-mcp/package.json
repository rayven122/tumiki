{
  "name": "@tumiki/google-calendar-mcp",
  "version": "0.1.0",
  "description": "Google Calendar MCP サーバー - Googleカレンダーの読み書き、管理、スケジューリング機能",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --incremental false && tsc-alias",
    "clean": "git clean -xdf .cache .turbo node_modules dist",
    "typecheck": "tsc --noEmit",
    "typecheck:dev": "tsgo --noEmit",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --check . --ignore-path ../../.gitignore --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.1",
    "dotenv": "^16.4.5",
    "google-auth-library": "^9.11.0",
    "googleapis": "^144.0.0",
    "zod": "catalog:",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@tumiki/eslint-config": "workspace:*",
    "@tumiki/prettier-config": "workspace:*",
    "@tumiki/tsconfig": "workspace:*",
    "@tumiki/vitest-config": "workspace:*",
    "@types/node": "^22.15.3",
    "@typescript/native-preview": "7.0.0-dev.20250814.1",
    "@vitest/coverage-v8": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsc-alias": "^1.8.16",
    "typescript": "catalog:",
    "typescript-eslint": "^8.33.0",
    "vitest": "catalog:"
  },
  "prettier": "@tumiki/prettier-config"
}

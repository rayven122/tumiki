{
  "name": "@tumiki/keycloak",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc --project tsconfig.build.json",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "test": "vitest",
    "migrate:role-subgroups": "dotenv -e ../../.env -- tsx src/scripts/migrateOrganizationRoleSubgroups.ts",
    "migrate:role-subgroups:dry-run": "DRY_RUN=true dotenv -e ../../.env -- tsx src/scripts/migrateOrganizationRoleSubgroups.ts"
  },
  "dependencies": {
    "@keycloak/keycloak-admin-client": "^26.0.7",
    "@tumiki/db": "workspace:*",
    "dotenv-cli": "^11.0.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@tumiki/eslint-config": "workspace:*",
    "@tumiki/prettier-config": "workspace:*",
    "@tumiki/tsconfig": "workspace:*",
    "@tumiki/vitest-config": "workspace:*",
    "@types/node": "^22.19.10",
    "@typescript/native-preview": "7.0.0-dev.20250814.1",
    "@vitest/coverage-v8": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsx": "^4.21.0",
    "typescript": "catalog:",
    "typescript-eslint": "^8.48.1",
    "vitest": "catalog:"
  },
  "prettier": "@tumiki/prettier-config"
}

{
  "name": "@tumiki/utils",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js"
    },
    "./server/security": {
      "types": "./dist/server/security/index.d.ts",
      "import": "./dist/server/security/index.mjs",
      "require": "./dist/server/security/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --check . --ignore-path ../../.gitignore --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:mcp-scan": "tsx src/server/security/__tests__/test-mcp-scan-integration.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.1",
    "@tumiki/db": "workspace:*",
    "next": "^15.3.3",
    "server-only": "^0.0.1",
    "uuid": "^10.0.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@tumiki/eslint-config": "workspace:*",
    "@tumiki/prettier-config": "workspace:*",
    "@tumiki/tsconfig": "workspace:*",
    "@tumiki/vitest-config": "workspace:*",
    "@types/uuid": "^10.0.0",
    "@typescript/native-preview": "7.0.0-dev.20250814.1",
    "@vitest/coverage-v8": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "prettier": "@tumiki/prettier-config"
}

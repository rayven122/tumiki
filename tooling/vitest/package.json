{
  "name": "@tumiki/vitest-config",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./base": {
      "types": "./base.config.d.ts",
      "default": "./base.config.js"
    },
    "./react": {
      "types": "./react.config.d.ts",
      "default": "./react.config.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --check . --ignore-path ../../.gitignore"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "vitest": "catalog:"
  },
  "devDependencies": {
    "@tumiki/eslint-config": "workspace:*",
    "@tumiki/prettier-config": "workspace:*",
    "@tumiki/tsconfig": "workspace:*",
    "@types/node": "^22.15.3",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@tumiki/prettier-config"
}
